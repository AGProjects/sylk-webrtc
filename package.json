{
  "name": "Sylk",
  "version": "2.1.0",
  "license": "AGPLv3",
  "private": true,
  "devDependencies": {
    "animate.css": "^3.5.2",
    "audio-context": "^1.0.3",
    "audio-loader": "^1.0.3",
    "autocomplete.js": "^0.28.3",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "bootstrap-css-only": "^3.3.7",
    "browser-sync": "^2.26.3",
    "browserify": "16.2.3",
    "classnames": "^2.2.6",
    "connect-modrewrite": "^0.10.2",
    "debug": "^3.2.6",
    "electron": "^3.1.6",
    "electron-builder": "^20.39.0",
    "envify": "^4.1.0",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-react-hooks": "^1.5.1",
    "fontawesome-actions": "^0.17.0",
    "gulp": "~3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-clean-css": "^3.9.0",
    "gulp-cli": "^2.2.0",
    "gulp-concat": "*",
    "gulp-eslint": "^3.0.1",
    "gulp-html-replace": "^1.6.2",
    "gulp-if": "*",
    "gulp-notify": "^3.2.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-terser": "^1.1.7",
    "gulp-useref": "3.1.6",
    "gulp-util": "^3.0.8",
    "hark": "^1.1.6",
    "ipaddr.js": "^1.9.0",
    "lazypipe": "^1.0.2",
    "localforage": "^1.7.3",
    "material-ui": "1.0.0-beta.40",
    "moment": "^2.24.0",
    "moment-duration-format": "^1.3.0",
    "murmurhash-js": "^1.0.0",
    "node-sass": "^4.9.3",
    "notifyjs": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-bootstrap": "^0.32.3",
    "react-dom": "^16.8.4",
    "react-mixin": "^4.0.0",
    "react-notification-system": "^0.2.17",
    "react-router-component": "^0.39.1",
    "react-transition-group": "^2.6.0",
    "require-dir": "^0.3.2",
    "sass-lint": "^1.12.1",
    "screenfull": "^4.0.1",
    "superagent": "^3.8.1",
    "sylkrtc": "AGProjects/sylkrtc.js",
    "underscore": "1.8.3",
    "uuid": "^3.1.0",
    "vinyl-buffer": "1.0.1",
    "vinyl-source-stream": "1.1.0",
    "watchify": "^3.11.1",
    "webrtc-adapter": "4.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron ./app",
    "build-osx": "electron-builder --mac --x64",
    "build-win": "electron-builder --win --ia32 --x64",
    "build-linux": "electron-builder --linux --ia32 --x64"
  },
  "build": {
    "appId": "com.agprojects.Sylk",
    "productName": "Sylk",
    "directories": {
      "output": "dist-electron"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://download.ag-projects.com/Sylk"
      }
    ],
    "mac": {
      "category": "public.app-category.video",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ],
      "signingHashAlgorithms": []
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false
    },
    "linux": {
      "category": "Audio;Video;AudioVideo",
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "target": [
        "AppImage"
      ]
    }
  }
}
